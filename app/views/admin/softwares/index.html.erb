<h1>Listing admin_softwares</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Language</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_softwares.each do |s| %>
  <tr>
    <td><%= s.title %></td>
    <td><%= s.description %></td>
    <td><%= s.language %></td>
    <td><%= link_to 'Show', admin_software_path(s) %></td>
    <td><%= link_to 'Edit', edit_admin_software_path(s) %></td>
    <td><%= link_to 'Destroy', admin_software_path(s), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

